<odoo>
    <data>
    <!-- Demo partners -->
        <record model="res.partner" id="ipx_sales_approval.partner_sales_approval_assistant">
            <field name="name">Sales Approval Assistant</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="email">saa@example.com</field>
            <field name="image_1920" type="base64" file="base/static/img/avatar_grey.png"/>
        </record>
        <record model="res.partner" id="ipx_sales_approval.partner_sales_approval_manager">
            <field name="name">Sales Approval Manager</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="email">sam@example.com</field>
            <field name="image_1920" type="base64" file="base/static/img/avatar_grey.png"/>
        </record>

    <!-- Demo users -->
        <record id="ipx_sales_approval.user_sales_approval_assistant" model="res.users">
            <field name="login">sales_approval_assistant</field>
            <field name="password">sales_approval_assistant</field>
            <field name="partner_id" ref="ipx_sales_approval.partner_sales_approval_assistant"/>
            <field name="company_id" ref="base.main_company" />
            <field name="company_ids" eval="[(4, ref('base.main_company'))]" />
            <field name="groups_id" eval="[(4,ref('ipx_sales_approval.sales_approval_assistant'))]" />
        </record>
        <record id="ipx_sales_approval.user_sales_approval_manager" model="res.users">
            <field name="login">sales_approval_manager</field>
            <field name="password">sales_approval_manager</field>
            <field name="partner_id" ref="ipx_sales_approval.partner_sales_approval_manager"/>
            <field name="company_id" ref="base.main_company" />
            <field name="company_ids" eval="[(4, ref('base.main_company'))]" />
            <field name="groups_id" eval="[(4,ref('ipx_sales_approval.sales_approval_manager'))]" />
        </record>
    </data>
</odoo>
